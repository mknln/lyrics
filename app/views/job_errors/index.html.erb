
<table>
  <tr>
    <th>Job</th>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
  </tr>

<% @job_errors.each do |job_error| %>
  <tr>
    <td><%= link_to 'Job', queue_job_path(job_error.queue_job_id) %></td>
    <td><%= job_error.title %></td>
    <td><%= job_error.description %></td>
    <td><%= link_to 'Show', job_error %></td>
    <td><%= link_to 'Destroy', job_error, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

